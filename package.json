{
  "name": "side-props-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate --config prisma/prisma.config.ts",
    "db:push": "prisma db push --config prisma/prisma.config.ts",
    "db:migrate": "prisma migrate dev --config prisma/prisma.config.ts",
    "db:studio": "prisma studio --config prisma/prisma.config.ts",
    "build:start": "npm run build && npm run start"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/adapter-neon": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.6.0",
    "jose": "^5.9.6",
    "next": "16.1.1",
    "prisma": "^7.3.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "typescript": "^5"
  }
}
